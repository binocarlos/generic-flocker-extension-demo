{
  "id":"app",
  "mem": 128,
  "cpus":0.1,
  "env":{
    "FILE":"/data/testdata.txt"
  },
  "container":{
    "type":"DOCKER",
    "docker":{
      "image":"binocarlos/hello-increment:latest",
      "network": "BRIDGE",
      "portMappings": [{
        "containerPort": 8000,
        "hostPort": 8000,
        "protocol": "tcp"
      }]
    },
    "volumes":[{
      "containerPath": "/data",
      "hostPath": "flocker/testdata",
      "mode": "RW"
    }],
    "forcePullImage":true
  },
  "ports":[
    8000
  ],
  "constraints": [["disk", "CLUSTER", "spinning"]],
  "cmd":""
}